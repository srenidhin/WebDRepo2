    <script src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.4.0/Chart.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap-select@1.13.9/dist/js/bootstrap-select.min.js"></script>
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <style>

        .navbar {
            transition: all 0.4s; 
            background-color:#222629;
        }
        .navbar .textNav{
            color:#61892F;
            text-transform:uppercase;
        }
        .navbar .nav-link {
            color: #fff;
        }
        .navbar .textNav:hover,
        .navbar .textNav:focus {
            background-color:#6B6E70;
            color: #86C232;
            text-decoration: none;
        }

        .navbar .navbar-brand {
            color: #86C232;
        }

         /*Change navbar styling on scroll*/
            .navbar.active {
                background: #474B4F;
                box-shadow: 1px 2px 10px rgba(0, 0, 0, 0.8);
            }

        .navbar.active .textNav:hover,
        .navbar.active .textNav:focus {
            background-color:#6B6E70;
            color: #86C232;
            text-decoration: none;
        }

        .navbar.active .textNav{
            color:#61892F;
        }

        .navbar.active .navbar-brand {
            color: #F79E02;
            
        }

        @@media (max-width: 991.98px) {
            .navbar {
                background: #fff;
            }

            .navbar .navbar-brand, .navbar .nav-link {
                
                
            }
        }

        .text-small {
            font-size: 0.9rem !important;
        }


        body {
            min-height: 110vh;
            background-color:white;
            height:100%
        }

        .textNav{
            color:#000;
            font-family: 'Montserrat', sans-serif;
            font-weight:bold;
            
        }

        .pagination a:hover{
            background-color:#2196F3;
        }

        .pagination a {
            border: solid;
          color: white;
          padding: 8px 16px;
          text-decoration: none;
        }
        .header{
            font-weight:800;
            font-style:italic;
            font-size:30px;
            color:#61892F;
            padding-top:30px;
            padding-left:30px;
            padding-bottom:30px;
        }
        .subheader{
            font-weight:800;
            font-style:italic;
            font-size:24px;
            color:#61892F;
            padding-left:30px;
        }
        .dropdwn {
          
          overflow: hidden;
        }
        
        .dropdown-content {
          display: none;
          position: absolute;
          background-color: #f9f9f9;
          min-width: 160px;
          box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
          z-index: 1;
        }

        .dropdown-content a {
          float: none;
          color: black;
          padding: 12px 16px;
          text-decoration: none;
          display: block;
          text-align: left;
        }

        .dropdown-content a:hover {
          background-color: #fff;
        }

        .dropdwn:hover .dropdown-content {
          display: block;
        }

 
    </style>
    <script>
        $(function () {
            $(window).on('scroll', function () {
                if ($(window).scrollTop() > 10) {
                    $('.navbar').addClass('active');
                } else {
                    $('.navbar').removeClass('active');
                }
            });
        });
    </script>
</head>
<body style="font-family: 'Montserrat', sans-serif;background-color:#222629">
    <header class="navbar-fixed-top">
        <nav class="navbar">
            <div class="container-fluid">
                <div class="navbar-header">
                    <a class="navbar-brand" style="font-family: 'Montserrat', sans-serif;font-size:35px;font-weight:900;font-style:italic;" href="#">Company A2Z</a>
                </div>
                <ul class="nav navbar-right" id="inside">
                    <li class="active"><a class="textNav" href="@Url.Action("DashBoard", "Home")">Home</a></li>
                    <li><a class="textNav" href="@Url.Action("UserMan", "Home")">User Management</a></li>
                    <li class="textNav">
                        <div class="dropdwn" style="padding:10px">
                            <a class="dropbtn">Excel Manipulation<i class="fa fa-caret-down" style="margin-left:5px"></i></a>
                            <div class="dropdown-content">
                                <a class="textNav" href="@Url.Action("AddFile", "Home")">Add File</a>
                                <a class="textNav" href="@Url.Action("EditTable", "Home")">Edit File</a>
                                <a class="textNav" href="@Url.Action("Index", "Sam")">Quick Edit</a>
                            </div>
                        </div>
                    </li>
                    <li class="textNav">
                        <div class="dropdwn" style="padding:10px">
                            <a class="dropbtn">Reports<i class="fa fa-caret-down" style="margin-left:5px"></i></a>
                            <div class="dropdown-content">
                                <a class="textNav" href="@Url.Action("UR", "Home")">User reports</a>
                                <a class="textNav" href="@Url.Action("FR", "Home")">File reports</a>
                            </div>
                        </div>
                    </li>
                    <li><a class="textNav" href="@Url.Action("Login", "Home")">Logout</a></li>
                </ul>
            </div>
        </nav>
    </header>
